                     </HBox.margin>
                     <image>
                        <Image url="@../../../../../../../../Desktop/TEC%20PROG/map_north_american.png" />
                     </image>
                  </ImageView>
                  <VBox prefHeight="196.0" prefWidth="315.0">
                     <children>
                        <HBox prefHeight="41.0" prefWidth="257.0">
                           <children>
                              <Label prefHeight="17.0" prefWidth="94.0" text="Selec Nerc">
